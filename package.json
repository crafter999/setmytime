{
  "name": "syncmytime",
  "version": "0.8.3",
  "description": "Sync & set the date of any Unix system from NTP public servers",
  "main": "./src/main.js",
  "bin": {
    "syncmytime": "./src/main.js"
  },
  "scripts": {
    "prep": "rm -rf ./dist && tsc && cp README.md ./dist && cp ./package* ./dist && rm -rf ./node-gyp/build && cp ./node-gyp/* ./dist/node-gyp",
    "install": "cd ./node-gyp && node-gyp rebuild "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crafter999/syncmytime.git"
  },
  "keywords": [
    "ntp",
    "client",
    "linux",
    "system",
    "date",
    "time"
  ],
  "author": "crafter999",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/crafter999/syncmytime/issues"
  },
  "homepage": "https://github.com/crafter999/syncmytime#readme",
  "devDependencies": {
    "@types/node": "^14.17.3"
  },
  "dependencies": {},
  "gypfile": true
}
